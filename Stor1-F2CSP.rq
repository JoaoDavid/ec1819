PREFIX : <http://www.w3.org>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
SELECT ?V1 ?V2 ?V3 ?V4 
WHERE {
	:D0 :values ?V1.
	:D0 :values ?V2.
	:D0 :values ?V3.
	:D0 :values ?V4.
	FILTER (
		( (?V1 != 0 || ?V2 != 0 || ?V3 != 1) )
		&& 
		( (?V1 != 1 || ?V2 != 1 || ?V3 != 0 || ?V4 != 0) )
	)
}