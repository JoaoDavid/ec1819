PREFIX : <http://www.w3.org>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
SELECT ?V1 ?V2 ?V3 ?V4 
WHERE {
	:D1 :values ?V1.
	:D2 :values ?V2.
	:D3 :values ?V3.
	:D3 :values ?V4.
	FILTER (
		( (?V1 != 0 || ?V2 != 2 || ?V3 != 4) )
		&& 
		( (?V1 != 1 || ?V2 != 3 || ?V3 != 5 || ?V4 != 4) )
	)
}